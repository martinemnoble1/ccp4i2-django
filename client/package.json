{
  "name": "ccp4x",
  "main": "main.cjs",
  "type": "module",
  "version": "0.0.1",
  "description": "An overlay of Electron/NextJS (frontend) and Django (backend) for CCP4X",
  "author": "PaulBond, Maritn Noble, Stuart McNicholas",
  "scripts": {
    "dev": "next dev",
    "update": "npx npm-check-updates -u",
    "start": "electron -r ts-node/register ./main.cjs",
    "electron-dev": "concurrently \"next dev\" \"npx ts-node ./node_modules/.bin/electron .\"",
    "electron-build": "next build && next export && npx ts-node/register ./node_modules/.bin/electron .",
    "package": "electron-builder",
    "compile": "tsc"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.1.6",
    "@mui/x-tree-view": "^7.26.0",
    "@types/chartjs": "^0.0.31",
    "chart.js": "^4.4.8",
    "chartjs-plugin-annotation": "^3.1.0",
    "concurrently": "^9.1.2",
    "express": "^4.21.2",
    "faker": "^6.6.6",
    "fs": "^0.0.1-security",
    "get-port": "^7.1.0",
    "get-port-please": "^3.1.2",
    "jquery": "^3.7.1",
    "jquery.flot": "^0.8.3",
    "moorhen": "^0.16.3",
    "mtz": "^0.1.0",
    "next": "^15.0.2",
    "pako": "^2.1.0",
    "plotly.js-dist-min": "^2.35.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.7",
    "swr": "^2.2.5",
    "uuid": "^11.0.5",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@types/node": "22.8.7",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/xml2js": "^0.4.14",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "xml-js": "^1.6.11"
  },
  "build": {
    "extraResources": [
      {
        "from": "../server/",
        "to": "server"
      }
    ]
  }
}
